#=============================================================================
# Configuration file to execute Heat tests.
#=============================================================================

[auth]
user_name={username of the cloud account}
api_key={api key of the cloud account: use either api key or password}
password={password of the cloud account: use either api key or password}
base_url=https://identity.api.rackspacecloud.com/v2.0

[heat]
base_url={heat endpoint to use;For example, https://api.dev.rs-heat.com/v1/}
template_url={template to use, give the raw template url}
